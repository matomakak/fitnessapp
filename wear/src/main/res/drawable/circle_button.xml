<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_enabled="false">
    <shape xmlns:android="http://schemas.android.com/apk/res/android"
      android:shape="oval">
      <solid android:color="#261d1c"/>
      <stroke android:color="#1a1a1a" android:width="18dip"/>
      <size android:height="60dp" android:width="60dp"/>
    </shape>
  </item>
  <item android:state_pressed="true">
    <shape xmlns:android="http://schemas.android.com/apk/res/android"
      android:shape="oval">
      <solid android:color="#ff0d00"/>
      <stroke android:color="#99a2ad" android:width="18dip"/>
      <size android:height="60dp" android:width="60dp"/>
    </shape>
  </item>
  <item android:state_pressed="false">
    <shape xmlns:android="http://schemas.android.com/apk/res/android"
      android:shape="oval">
      <solid android:color="#974743"/>
      <stroke android:color="#99a2ad" android:width="18dip"/>
      <size android:height="60dp" android:width="60dp"/>
    </shape>
  </item>
</selector>